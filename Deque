# Ejemplo deque
from collections import deque

# Inicializa lista
numbers=deque()

# Utiliza append para ingresar valor
numbers.append(99)
numbers.append(15)
numbers.append(82)
numbers.append(50)
numbers.append(47)

# Como pop
last_item=numbers.pop()
print(last_item)
print(numbers)

# Como cola
first_item=numbers.popleft()
print(last_item)
print(numbers)
